<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>SentenceTokenTypeSystemDescriptor</name>
  <description>A test type system for sentence and token annotation.
Q: Need an example of using ResourceManager to bundle type system descriptors and POJOs and then move around projects?</description>
  <version>1.0</version>
  <vendor/>
  <types>
    <typeDescription>
      <name>dev.amb.uima.typeSystem.core.Token</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>sentStartPos</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>sentEndPos</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>dev.amb.uima.typeSystem.core.Sentence</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>dev.amb.uima.typeSystem.ner.Person</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>corefMentions</name>
          <description/>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>dev.amb.uima.typeSystem.coref.RefMention</elementType>
          <multipleReferencesAllowed>true</multipleReferencesAllowed>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>dev.amb.uima.typeSystem.coref.RefMention</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>dev.amb.uima.typeSystem.coref.CorefDiscourseEntity</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>corefMentions</name>
          <description/>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>dev.amb.uima.typeSystem.coref.RefMention</elementType>
          <multipleReferencesAllowed>true</multipleReferencesAllowed>
        </featureDescription>
        <featureDescription>
          <name>numberOfMentions</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
